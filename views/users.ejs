    <div style="margin-top: 20px;">
        <img src="/images/qq.png" alt="" onclick="window.open('<%= root%>/auth/qq')" style="cursor: pointer;width: 50px;height: 50px;margin-right: 10px;">
        <img src="/images/line.png" alt="">
        <img src="/images/weixin.png" alt="" onclick="window.open('<%= root%>/auth/godutch')" style="cursor: pointer;width: 50px;height: 50px;margin-left: 10px">
